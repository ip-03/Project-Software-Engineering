<Window x:Class="WeatherApp.MoreDataWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WeatherApp"
        mc:Ignorable="d"
        Title="MoreDataWindow" Height="450" Width="940">
    <Grid>
        <StackPanel Margin="10,10,0,10">
        </StackPanel>
        <TextBlock Text="WeatherApp" FontSize="24" FontWeight="Bold" Foreground="Black"/>
        <DataGrid x:Name="dataGrid" AutoGenerateColumns="False" Margin="0,35,10,10"/>
    </Grid>
</Window>
